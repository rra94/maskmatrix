{
    "system": {
        "dataset": "MSCOCO",
        "batch_size":1,
        "sampling_function": "anchors_detection_matrixnets",
	"model_name": "MatrixNetAnchors",

        "train_split": "val2017",
        "val_split": "val2017",

        "learning_rate": 0.00005,
        "decay_rate": 10,

        "val_iter": 1000,

        "opt_algo": "adam",
        "prefetch_size":6,

        "max_iter": 150000,
        "stepsize": 130000,
        "snapshot": 500,

        "chunk_sizes":[1],
	"cache_dir" : "./MatrixNetAnchorsResnet50_FPN_48LayerRange_640isize",
	"data_dir": "./data/"
    },

    "db": {
        "rand_scale_min": 1.0,
        "rand_scale_max": 1.1,
        "rand_scale_step": 0.1,
        "rand_scales": null,
	"num_dets":2000,
        "rand_crop": true,
        "rand_color": false,

        "border": 128,
        "gaussian_bump": true,
        "gaussian_iou": 0.3,
        
        "input_size": [640, 640],
   
       "output_kernel_size": 1 ,
       "base_layer_range":[24,48,24,48],
       "layers_range": [[[0,48,0,48],-1,-1, -1, -1],
                 [-1,[48,96,48,96],-1,-1, -1],
                 [-1,-1,[96,192,96,192],-1, -1],
                 [-1, -1, -1,[192,384,192,384],-1],
                 [-1, -1, -1, -1 ,[384,2000,384,2000]]] ,
        
        
	
        "test_scales": [1],
	
        "categories":81,
	"test_image_max_dim":640,
	"test_flip_images":false,
        "nms_threshold": 0.5,
        "max_per_image": 500,
        "merge_bbox": false,
        "weight_exp": 8,
	"cutout":false,
        "top_k": 2000,
        "backbone": "resnet50"
    }
}
